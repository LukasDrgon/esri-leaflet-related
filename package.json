{
  "name": "esri-leaflet-related",
  "description": "A Leaflet plugin that allows users to query related tables.",
  "version": "2.0.0",
  "author": "John Gravois <jgravois@esri.com> (http://johngravois.com)",
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "whitelist": [
            "es6.modules"
          ],
          "loose": [
            "es6.modules"
          ]
        }
      ]
    ]
  },
  "contributors": [
    "John Gravois <jgravois@esri.com> (http://johngravois.com)"
  ],
  "dependencies": {
    "leaflet": "^1.0.0-beta.1",
    "esri-leaflet": "^2.0.0-beta.4"
  },
  "devDependencies": {
    "babelify": "^6.1.3",
    "chai": "2.3.0",
    "gh-release": "^2.0.0",
    "isparta": "^3.0.3",
    "istanbul": "gotwarlost/istanbul.git#source-map",
    "karma": "^0.12.24",
    "karma-chai-sinon": "^0.1.3",
    "karma-coverage": "douglasduteil/karma-coverage#next",
    "karma-mocha": "^0.1.0",
    "karma-mocha-reporter": "^0.2.5",
    "karma-phantomjs-launcher": "^0.1.4",
    "karma-sourcemap-loader": "^0.3.5",
    "mkdirp": "^0.5.1",
    "node-sass": "^3.2.0",
    "phantomjs": "^1.9.17",
    "rollup": "^0.10.0",
    "semistandard": "^6.1.2",
    "sinon": "^1.11.1",
    "sinon-chai": "2.7.0",
    "uglify-js": "^2.4.23"
  },
  "homepage": "https://github.com/jgravois/esri-leaflet-related",
  "jsnext:main": "src/EsriLeafletRelated.js",
  "jspm": {
    "files": [
      "src/*.js",
      "dist/*"
    ],
    "registry": "npm",
    "format": "es6"
  },
  "license": "Apache-2.0",
  "main": "src/EsriLeafletRelated.js",
  "readmeFilename": "README.md",
  "repository": {
    "type": "git",
    "url": "git@github.com:jgravois/esri-leaflet-related.git"
  },
  "scripts": {
    "prebuild": "mkdirp dist",
    "build": "./scripts/build.js",
    "lint": "semistandard src/*.js",
    "prepublish": "npm run build",
    "pretest": "npm run build",
    "release": "./scripts/release.sh",
    "test": "npm run lint && karma start"
  }
}